    <fields>
        <fullName>Additional_Commission_Percent__c</fullName>
        <externalId>false</externalId>
        <formula>IF(DATEVALUE(CreatedDate) &lt; DATEVALUE(&apos;2015-12-16&apos;), 
IF(
  AND(NOT(ISNULL(System_Design_Quote__r.Community_Program_Offer__c)), NOT(ISBLANK(System_Design_Quote__r.Community_Program_Offer__c))), 
  0.00,
  IF(
  OR(
    AND(ISPICKVAL(OpportunityId__r.LeadSource, &apos;Individual Referral&apos;),   OpportunityId__r.Referring_Account__r.OwnerId = AccountId__r.OwnerId),
    AND(ISPICKVAL(OpportunityId__r.LeadSource, &apos;Partner Referral&apos;), OpportunityId__r.Referring_Account__r.OwnerId = Original_Submit_Sales_Rep__c),
    ISPICKVAL(OpportunityId__r.LeadSource, &apos;Event&apos;)
  ), 
  0.01,
  IF(AND(OpportunityId__r.Self_Generated__c, NOT(ISNULL(OpportunityId__r.Contact_Detail__c)), NOT(ISBLANK(OpportunityId__r.Contact_Detail__c))), 0.02, 0.00))
), 0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Additional Commission (%)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>4</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
