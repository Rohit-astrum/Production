    <fields>
        <fullName>Rebate_Status__c</fullName>
        <description>Status for Project Compliance to track PM Project process stages.</description>
        <externalId>false</externalId>
        <formula>IF( ISNULL( Grant_Reservation_Submitted__c ), &quot;0-Reservation Not Submitted&quot;,

 IF( AND( ISNULL( Grant_Documents__c ),  NOT( ISNULL( Grant_Reservation_Submitted__c ))), &quot;1-Reservation Submitted, Not Approved&quot;,

 IF( AND( ISNULL( Grant_Completion_Completed__c ),  NOT( ISNULL( Grant_Documents__c ))), &quot;2-Reserved, Completion Not Posted&quot;,

 IF( AND( ISNULL( Work_Completed__c ),  NOT( ISNULL( Grant_Documents__c )),  NOT( ISNULL( Grant_Completion_Completed__c ))), &quot;3-Reserved, Work Not Complete&quot;,

 IF( AND( ISNULL( Grant_Completion_Submitted__c ),  NOT( ISNULL( Grant_Documents__c )),  NOT( ISNULL( Grant_Completion_Completed__c )),  NOT( ISNULL( Work_Completed__c ))), &quot;4-Installed, Completion 1 Not Submitted&quot;,

 IF( AND( ISNULL( Grant_Completion__c ),  NOT( ISNULL( Grant_Completion_Submitted__c ))), &quot;5-Completion 1 Submitted, Not Approved&quot;, 

 IF( AND( ISNULL( Approved_Utility_Interconnect_Part_2__c ),  NOT( ISNULL( Grant_Completion_Submitted__c )),  NOT( ISNULL( Grant_Completion__c ))), &quot;6-Completion 1 Approved, Awaiting PTO&quot;,

 IF( AND( ISNULL( Grant_Completion_Submitted_Part_2__c ),  NOT( ISNULL( Grant_Completion__c )),  NOT( ISNULL(  Approved_Utility_Interconnect_Part_2__c ))), &quot;7-PTO, Completion 2 Not Submitted&quot;,

 IF( AND( ISNULL( Grant_Completion_Approved_Part_2__c ),  NOT( ISNULL( Grant_Completion_Submitted_Part_2__c ))), &quot;8-Completion 2 Submitted, Not Approved&quot;,

 IF( NOT( ISNULL( Grant_Completion_Approved_Part_2__c )), &quot;9-Grant Complete&quot;, &quot;&quot; ))))))))))</formula>
        <label>Rebate Status</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
