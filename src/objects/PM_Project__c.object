IF( AND( ISNULL( Lease_Contract_Approval__c ), NOT( ISNULL( Lease_Contract_Submission__c ) ) ), &quot;2. Contract Approval&quot;,

IF( AND( ISNULL( Lease_Design_Submitted__c ), NOT( ISNULL( Lease_Contract_Approval__c ) ) ), &quot;3. Design Submission&quot;,

IF( AND( ISNULL( Lease_Design__c ), NOT( ISNULL( Lease_Design_Submitted__c ) ) ), &quot;4. Design Approval&quot;,

IF( AND( ISNULL( Work_Completed__c ), NOT( ISNULL( Lease_Design__c ) ) ), &quot;5. Pending Work Complete&quot;,

IF( AND( ISNULL( Lease_Substantially_Submitted__c ), NOT( ISNULL( Work_Completed__c ) ) ), &quot;6. Substantial Submission&quot;,

IF( AND( ISNULL( Lease_Substantially_Complete__c ), NOT( ISNULL(  Lease_Substantially_Submitted__c ) ) ), &quot;7. Substantial Approval&quot;,

IF( AND( ISNULL( Lease_Final_Package_Submission__c ), NOT( ISNULL(  Lease_Substantially_Complete__c ) ) ), &quot;8. Final Submission&quot;,

IF( AND( ISNULL( Lease_Final_Package_Approved__c ), NOT( ISNULL(  Lease_Final_Package_Submission__c ) ) ), &quot;9. Final Approval&quot;,

IF( AND( NOT( ISNULL(  Lease_Final_Package_Approved__c ) ) ), &quot;10. Finance Complete&quot;,

&quot;&quot;)))))))))))</formula>
