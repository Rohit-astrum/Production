    <fields>
        <fullName>IR2_Vendor_Invoice_Num__c</fullName>
        <externalId>false</externalId>
        <formula>BlankValue(Ref_Number__c, TrxID__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
