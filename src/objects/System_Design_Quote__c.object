    <fields>
        <fullName>Scenario_4_Site_Quality__c</fullName>
        <externalId>false</externalId>
        <formula>/* Original spec:
(if Scenario N Lease Fund = ‘NRG’ then NRG Overall Site Quality, else if Scenario N Lease Fund = ‘Sunnova’ then Sunnova Overall Site Quality, else if Scenario N Lease Fund = ‘CT Lease’ or ‘CPF’ then Overall Site Quality, else if Scenario N Financing Type = ‘Purchase’ and there is only one other scenario with Financing Type = ‘Lease’ then site quality of Lease, else Overall Site Quality)
*/

IF(ISPICKVAL(Scenario_4_Financing_Type__c ,&apos;Purchase&apos;)
  ,IF(  /* If there is only one other scenario with Financing Type = ‘Lease’ */    
     Scenario_4_Is_Purchase_Single_Lease__c 
    /* then site quality of Lease */
    ,IF(ISPICKVAL(Scenario_2_Financing_Type__c, &apos;Lease&apos;) ,Scenario_2_Lease_Site_Quality__c
      ,IF(ISPICKVAL(Scenario_3_Financing_Type__c,&apos;Lease&apos;) ,Scenario_3_Lease_Site_Quality__c
        ,IF(ISPICKVAL(Scenario_1_Financing_Type__c ,&apos;Lease&apos;) ,Scenario_1_Lease_Site_Quality__c
          ,NULL
        ) 
      ) 
    )
    /* else Overall Site Quality */
    , System_Design__r.Overall_Site_Quality__c 
  )
  /* Non purchase, so default to the Lease site quality calculation */
  ,Scenario_4_Lease_Site_Quality__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Scenario 4 Site Quality</label>
        <precision>18</precision>
