        <formula>IF(ISPICKVAL(Scenario_4_Financing_Type__c ,&apos;Purchase&apos;)
  ,IF(  /* If there is only one other scenario with Financing Type = ‘Lease’ */    
     Scenario_4_Is_Purchase_Single_Lease__c 
    /* then projected output of Lease */
    ,IF(ISPICKVAL(Scenario_1_Financing_Type__c, &apos;Lease&apos;) ,Scenario_1_Lease_Projected_Annual_Output__c 
      ,IF(ISPICKVAL(Scenario_2_Financing_Type__c,&apos;Lease&apos;) ,Scenario_2_Lease_Projected_Annual_Output__c
        ,IF(ISPICKVAL(Scenario_3_Financing_Type__c ,&apos;Lease&apos;) ,Scenario_3_Lease_Projected_Annual_Output__c
          ,NULL
        ) 
      ) 
    )
    /* else Overall projected annual output */
    , System_Design__r.Total_Projected_Annual_Output_kWh_y__c
  )
