    <fields>
        <fullName>Electrician_Bonus_Budget_Hours__c</fullName>
        <description>Calculated Hours Budgeted for ELECTRICIAN Bonus Program</description>
        <externalId>false</externalId>
        <formula>$Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Intercept__c + Electrician_Param_Number_of_Panels__c * $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Num_Panels__c +  $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Num_Installers__c *  $Setup.AstrumTIME_Settings__c.CONST_BONUS_ELECTRIC_Electrician_Per_Job__c +  IF(CONTAINS(Electrician_Param_Baltimore_County__c,&apos;Yes&apos;), 1, 0) * $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Baltimore_County__c +  IF( CONTAINS(Electrician_Param_Inside_Only_Conduit__c,&apos;Yes&apos;) , 1, 0) * $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Inside_Only_Conduit__c +  IF( CONTAINS(Electrician_Param_Outbuilding_Install__c,&apos;Yes&apos;) , 1, 0) * $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Outbuilding_Install__c + Electrician_Param_Hours_to_Office__c * $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Distance_Hours__c + IF(CONTAINS(Electrician_Param_Flat_Roof__c,&apos;Yes&apos;), 1,0)* $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Flat_Roof__c + IF(CONTAINS(Electrician_Param_Ground_Mount__c,&apos;Yes&apos;), 1, 0) * $Setup.AstrumTIME_Settings__c.COEF_BONUS_ELECTRIC_Ground_Mount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Calculated Hours Budgeted for ELECTRICIAN Bonus Program</inlineHelpText>
        <label>Electrician Bonus Budget Hours</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
