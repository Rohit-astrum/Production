    <script type="text/javascript">
    
        function getURLParam(name) {
            return decodeURI(
                (RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,''])[1]
            );
        }
        
        var a_gclid = getURLParam('gclid');
        var a_utmSource = getURLParam('utm_source');
        var a_utmMedium = getURLParam('utm_medium');
        var a_utmTerm = getURLParam('utm_term');
        var a_utmCampaign = getURLParam('utm_campaign');
        var a_utmContent = getURLParam('utm_content');
        
        if (a_gclid!=''){
        $.cookie('apex__gclid', a_gclid, { expires: 30, path: '/' });
        }
        if (a_utmSource!=''){
        $.cookie('apex__desolar_utm_source', a_utmSource, { expires: 30, path: '/' });
        }
        if (a_utmMedium!=''){
        $.cookie('apex__desolar_utm_medium', a_utmMedium, { expires: 30, path: '/' });
        }
        if (a_utmTerm!=''){
        $.cookie('apex__desolar_utm_term', a_utmTerm, { expires: 30, path: '/' });
        }
        if (a_utmCampaign!=''){
        $.cookie('apex__desolar_utm_campaign', a_utmCampaign, { expires: 30, path: '/' });
        }
        if (a_utmContent!=''){
        $.cookie('apex__desolar_utm_content', a_utmContent, { expires: 30, path: '/' });
        }
    
    </script>
