    <script type="text/javascript">
      function initialize() {
          var image = '{!URLFOR($Resource.InstallDemoLandingPage_Resources, 'images/MAP_icon.png')}';
          var myLatlng = new google.maps.LatLng(40.980335, -74.034936);
          var mapOptions = {
            zoom: 15,
            center: myLatlng
          }
          var map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
        
          var marker = new google.maps.Marker({
              position: myLatlng,
              map: map,
              icon: image,
              title: 'We Are Going Solar!'
          });
        }
     </script>



    
    <script type="text/javascript">
    
        function getURLParam(name) {
            return decodeURI(
                (RegExp(name + '=' + '(.+?)(&|$)').exec(location.search)||[,''])[1]
            );
        }
        
        var a_gclid = getURLParam('gclid');

        var a_utmSource = getURLParam('utm_source');
        if(a_utmSource == '')
            a_utmSource = 'cmpt';
            
        var a_utmMedium = getURLParam('utm_medium');
        if(a_utmMedium == '')
            a_utmMedium = 'dm';
            
        var a_utmTerm = getURLParam('utm_term');

        var a_utmContent = getURLParam('utm_content');

        var a_utmCampaign = getURLParam('utm_campaign');
        if(a_utmCampaign == '')
            a_utmCampaign = 'indpanpa'
        
        if (a_gclid!=''){
        $.cookie('apex__gclid', a_gclid, { expires: 30, path: '/' });
        }
        if (a_utmSource!=''){
        $.cookie('apex__astrum_utm_source', a_utmSource, { expires: 30, path: '/' });
        }
        if (a_utmMedium!=''){
        $.cookie('apex__astrum_utm_medium', a_utmMedium, { expires: 30, path: '/' });
        }
        if (a_utmTerm!=''){
        $.cookie('apex__astrum_utm_term', a_utmTerm, { expires: 30, path: '/' });
        }
        if (a_utmCampaign!=''){
        $.cookie('apex__astrum_utm_campaign', a_utmCampaign, { expires: 30, path: '/' });
        }
        if (a_utmContent!=''){
        $.cookie('apex__astrum_utm_content', a_utmContent, { expires: 30, path: '/' });
        }
    
    </script>
