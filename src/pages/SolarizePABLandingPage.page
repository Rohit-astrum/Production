        //get the form field values
        varvals = form.getValues();
        
    
    });

//Add an onSuccess handler
    form.onSuccess(function() {
                form.getFormElem()[0].style.display = 'none';
                document.getElementById('confirmform').style.display = 'block';
                return false;
    });


});

