        //Fill in the hidden company field for SF
        $('[name="Company"]').val($("#LastName").val() + ', ' +  $("#FirstName").val());
        
        //Get the current date and insert into lead created date for SF
        var today = new Date();
        var dd = today.getDate();
        var mm = today.getMonth()+1; //January is 0!
