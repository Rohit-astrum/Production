    Property_County__c = &quot;Otsego&quot;, 
    Property_County__c = &quot;Otsego (NY)&quot;,
    Property_County__c = &quot;Tioga (NY)&quot;,
    Property_County__c = &quot;Tioga&quot; ), 

IF(AND( OR(ISPICKVAL(StageName,&quot;Closed - Lost&quot;),
           ISPICKVAL(StageName,&quot;Closed - Won&quot;),
           ISPICKVAL(StageName,&quot;Contract Cancelled&quot;)),

OR(CloseDate &lt;Date(2015,6,15),ISNULL(CloseDate))),

FALSE,TRUE))</formula>
