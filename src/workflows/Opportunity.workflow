    <rules>
        <fullName>Community Program Update -- CT Solarize Online %28Opportunity%29</fullName>
        <actions>
            <name>CP_Update_CT_Solarize_Online_Opp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>If Opportunity is located in South Windsor, CT or Lyme, CT or Old Lyme, CT or Woodstock, CT, update Community Program to &apos;CT Solarize Online&apos;</description>
        <formula>AND( 	OR( 		AND(Property__r.Property_City__c = &quot;South Windsor&quot;, Property_State__c  = &quot;CT&quot;), 		AND(Property__r.Property_City__c = &quot;Lyme&quot;, Property_State__c  = &quot;CT&quot;), 		AND(Property__r.Property_City__c = &quot;Old Lyme&quot;, Property_State__c  = &quot;CT&quot;), 		AND(Property__r.Property_City__c = &quot;Woodstock&quot;, Property_State__c  = &quot;CT&quot;) 	), 	IF( 		AND( 			OR(ISPICKVAL(StageName,&quot;Closed - Lost&quot;), ISPICKVAL(StageName,&quot;Closed - Won&quot;), ISPICKVAL(StageName,&quot;Contract Cancelled&quot;)), 			OR( 				OR( 					AND(Property__r.Property_City__c = &quot;South Windsor&quot;, Property_State__c  = &quot;CT&quot;, CloseDate &lt; Date(2015,03,17)), 					AND(Property__r.Property_City__c = &quot;Lyme&quot;, Property_State__c  = &quot;CT&quot;, CloseDate &lt; Date(2015,03,25)), 					AND(Property__r.Property_City__c = &quot;Old Lyme&quot;, Property_State__c  = &quot;CT&quot;, CloseDate &lt; Date(2015,04,10)), 					AND(Property__r.Property_City__c = &quot;Woodstock&quot;, Property_State__c  = &quot;CT&quot;, CloseDate &lt; Date(2015,04,09)) 				), 				OR(ISNULL(CloseDate)) 			) 		), 		FALSE,TRUE 	) )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
