    <rules>
        <fullName>Community Program -- Solarize Northern Dutchess %28Opp%29</fullName>
        <actions>
            <name>CP_Update_Solarize_No_Dutchess_Opp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update the community program field to Solarize Northern Dutchess
when the opportunity is in Dutchess county of NY</description>
        <formula>AND(Property_State__c = &quot;NY&quot;, QAS_Property_County__c = &quot;Dutchess &quot;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Community Program Update -- Andover %28Opportunity%29</fullName>
        <actions>
            <name>CP_Update_Andover_Opp</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <description>If Opportunity is located in Andover, MA, update Community Program to &apos;Solarize Andover&apos;</description>
        <formula>AND(Property__r.Property_City__c = &quot;Andover&quot;, Property_State__c  = &quot;MA&quot;,IF(AND( OR(ISPICKVAL(StageName,&quot;Closed - Lost&quot;), ISPICKVAL(StageName,&quot;Closed - Won&quot;), ISPICKVAL(StageName,&quot;Contract Cancelled&quot;)), OR(CloseDate &lt;Date(2014,1,27),ISNULL(CloseDate))),FALSE,TRUE))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Community Program Update -- Belmont Goes Solar %28Opportunity%29</fullName>
